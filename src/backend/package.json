{
  "name": "myteam-backend",
  "version": "1.0.0",
  "description": "MyTeam project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot --instances=1 services/**/*.*.js",
    "start": "moleculer-runner services/**/*.*.js",
    "service": "moleculer-runner --instances=1 services/**/*.service.js",
    "gateway": "moleculer-runner services/*.gateway.js",
    "cli": "moleculer connect TCP",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "moleculer-repl": "^0.6.2"
  },
  "dependencies": {
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "ldapjs": "^1.0.2",
    "moleculer": "^0.14.0",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.7",
    "moleculer-web": "^0.9.0",
    "redlock": "^4.1.0",
    "socket.io": "^2.3.0"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
